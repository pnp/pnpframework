{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/lib/PnP.Framework"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "using-the-framework/**.md",
          "using-the-framework/toc.yml",
          "contributing/**.md",
          "contributing/toc.yml",
          "tutorials/**.md",
          "tutorials/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
      // {
      //   "files": [
      //     "demos/**.md",
      //     "demos/**/**.md",
      //     "demos/**/toc.yml",
      //     "toc.yml",
      //     "*.md"
      //   ]
      // }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "demos/**/*.png",
          "tutorials/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "PnP Framework",
      "_appFooter": "<span>PnP Framework<br>Generated by <strong><a href='https://dotnet.github.io/docfx' alt='Doc FX Website'>DocFX</a></strong> with <strong><a href='https://ovasquez.github.io/docfx-material' alt='DocFx Material Theme'>Material UI</a></strong></span>",
      "_appFaviconPath": "https://c.s-microsoft.com/favicon.ico?v2",
      "_appLogoPath": "images/logo.svg",
      "_description":"The PnP Framework is a .NET Standard 2.0 / .NET 5.0 / .NET 6.0 / .NET 7.0 library targeting Microsoft 365 containing the PnP Provisioning engine and a ton of other useful extensions.",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  }
}
